{
  "name": "holly-super-agent",
  "version": "1.0.0",
  "description": "HOLLY - Hyper-Optimized Logic & Learning Yield - Autonomous AI Development Partner",
  "__IMPORTANT_NOTE__": "DO NOT upgrade tailwindcss to v4+ - it requires @tailwindcss/postcss. Keep at v3.4.1",
  "main": "index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "prisma generate && NODE_OPTIONS='--max-old-space-size=4096' next build",
    "vercel-build": "prisma generate && NODE_OPTIONS='--max-old-space-size=4096' next build",
    "vercel-build-with-architecture": "prisma db push --accept-data-loss && prisma generate && npx ts-node --transpile-only scripts/generate-architecture.ts && next build",
    "generate-architecture": "npx ts-node --transpile-only scripts/generate-architecture.ts",
    "start": "next start",
    "lint": "next lint",
    "scan": "node .holly/tools/comprehensive-scanner.js",
    "scan:basic": "node .holly/tools/typescript-scanner.js",
    "pre-deploy": "npm run scan && npx prisma validate",
    "safe-deploy": "npm run pre-deploy && npm run build",
    "test": "npx jest --passWithNoTests",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage --passWithNoTests",
    "setup:db": "ts-node setup-database.ts",
    "seed:db": "ts-node database-seed.ts",
    "migrate:db": "psql $DATABASE_URL -f database-schema.sql"
  },
  "keywords": [
    "ai",
    "development",
    "automation",
    "code-generation",
    "deployment",
    "github",
    "emotional-intelligence"
  ],
  "author": "Steve Hollywood Dorego",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@clerk/nextjs": "^5.0.0",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@huggingface/inference": "^4.13.5",
    "@octokit/rest": "^20.1.2",
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-avatar": "1.1.10",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-switch": "1.1.3",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-tooltip": "1.2.8",
